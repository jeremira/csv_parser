<!DOCTYPE html>
<html>
  <head>
    <title>CsvParser</title>
    <%= csrf_meta_tags %>

    <%= stylesheet_link_tag    'application', media: 'all', 'data-turbolinks-track': 'reload' %>
    <%= javascript_include_tag 'application', 'data-turbolinks-track': 'reload' %>
  </head>

  <body>
    <nav><%=link_to 'Import file', new_booking_data_path %><%= link_to 'See data', booking_data_path %></nav>

    <section class="flash-section">
      <% if notice %>
          <p><%= notice %></p>
      <% end %>
      <% if alert %>
          <p><%= alert %></p>
      <% end %>
    </section>

    <%= yield %>
  </body>
</html>
